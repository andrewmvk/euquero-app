{
  "expo": {
    "name": "Eu Quero",
    "slug": "euqueroapp",
    "version": "1.0.2",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B0F"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#fff"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.euquero.app"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "svg"
      ]
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}
